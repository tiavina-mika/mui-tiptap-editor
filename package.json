{
  "name": "mui-tiptap-editor",
  "version": "4.0.0",
  "description": "Easy to use Tiptap WYSIWYG rich text editor using Material UI (MUI) for React",
  "keywords": [
    "mui",
    "tiptap",
    "material-ui",
    "prosemirror",
    "rich text editor",
    "react text editor",
    "wysiwyg",
    "quill",
    "draftjs",
    "slate"
  ],
  "homepage": "https://github.com/tiavina-mika/mui-tiptap-editor",
  "bugs": {
    "url": "https://github.com/tiavina-mika/mui-tiptap-editor/issues"
  },
  "author": {
    "name": "Tiavina Michael Ralainirina",
    "email": "tiavinamika@gmail.com",
    "github": "https://github.com/tiavina-mika"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs"
    }
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite --config vite.config.ts dev",
    "build:dev": "vite build --config vite.config.ts",
    "build": "rollup -c rollup.config.js",
    "test": "jest",
    "test:ci": "jest --ci --coverage",
    "test:e2e": "playwright test --config=playwright.config.ts",
    "test:e2e:ui": "yarn test:e2e --ui",
    "test:e2e:headed": "yarn test:e2e --headed",
    "lint": "eslint . --ext .ts,.tsx --fix --max-warnings=1000",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "sb": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "md-link:check": "markdown-link-check -v -p README.md -v -p CONTRIBUTING.md -v -p .github/**/*.md -c .markdown-link-check-config.json",
    "spell:check": "cspell \"{README.md,CONTRIBUTING.md,.github/*.md}\"",
    "publish": "yarn build && yarn npm publish && yarn demos",
    "demos": "cd demos && yarn upgrade && rimraf ./demos/node_modules && yarn && yarn dev"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@tiptap/extension-bubble-menu": "^3.6.5",
    "@tiptap/extension-code": "^3.6.5",
    "@tiptap/extension-code-block-lowlight": "^3.6.5",
    "@tiptap/extension-floating-menu": "^3.6.5",
    "@tiptap/extension-heading": "^3.6.5",
    "@tiptap/extension-image": "^3.6.5",
    "@tiptap/extension-link": "^3.6.5",
    "@tiptap/extension-mention": "^3.6.5",
    "@tiptap/extension-table": "^3.6.5",
    "@tiptap/extension-table-of-contents": "^3.6.5",
    "@tiptap/extension-text": "^3.6.5",
    "@tiptap/extension-text-align": "^3.6.5",
    "@tiptap/extension-text-style": "^3.6.5",
    "@tiptap/extension-typography": "^3.6.5",
    "@tiptap/extension-underline": "^3.6.5",
    "@tiptap/extension-youtube": "^3.6.5",
    "@tiptap/extensions": "^3.6.5",
    "@tiptap/pm": "^3.6.5",
    "@tiptap/react": "^3.6.5",
    "@tiptap/starter-kit": "^3.6.5",
    "@tiptap/suggestion": "^3.6.5",
    "lowlight": "^3.1.0",
    "tippy.js": "^6.3.7"
  },
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@mui/material": "^7.3.4",
    "react": "^16.8.0 || ^17.0.2 || ^18.2.0 || ^19.2.0",
    "react-dom": "^16.8.0 || ^17.0.2 || ^18.2.0 || ^19.2.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.28.4",
    "@chromatic-com/storybook": "4.1.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@cspell/eslint-plugin": "^9.2.1",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.37.0",
    "@floating-ui/dom": "^1.6.0",
    "@mui/material": "^7.3.4",
    "@playwright/test": "^1.56.1",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^28.0.8",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-url": "^8.0.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.2",
    "@storybook/addon-a11y": "9.1.12",
    "@storybook/addon-docs": "9.1.12",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^9.1.12",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/addon-vitest": "9.1.12",
    "@storybook/jest": "^0.2.3",
    "@storybook/preset-create-react-app": "^9.1.12",
    "@storybook/react": "^9.1.12",
    "@storybook/react-vite": "9.1.12",
    "@storybook/testing-library": "^0.2.2",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@types/eslint-plugin-jsx-a11y": "^6",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/rollup-plugin-peer-deps-external": "^2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-loader": "^10.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cspell": "^9.2.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prefer-arrow-functions": "^3.9.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "9.1.12",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.3",
    "markdown-link-check": "^3.13.7",
    "playwright": "^1.56.1",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.4",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-import-css": "^4.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-preserve-use-client": "^3.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^6.0.4",
    "semantic-release": "^24.2.9",
    "storybook": "9.1.12",
    "ts-jest": "^29.4.5",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.9",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "yarn@4.10.3"
}
